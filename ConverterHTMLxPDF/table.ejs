<!DOCTYPE html>
<html lang="en">


<head>
    <title>table</title>
</head>


<table>
    <thead>
        <% header.forEach(col => {  %>
            <th> <%= col %></th>
        <% }) %>
    </thead>
    <tbody>
        <% rows.forEach(row => { %>
            <tr>
                <% row.forEach(element => { %>
                    <td><%= element %></td>
                <%})%>
            </tr>
        <%})%>
    </tbody>
</table>

</html>